{
  "name": "ollama-jupyter-ai",
  "version": "0.1.0",
  "description": "AI-powered JupyterLab extension using Ollama for local LLM integration",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "ollama",
    "ai",
    "assistant"
  ],
  "homepage": "https://github.com/yourusername/ollama-jupyter-ai",
  "bugs": {
    "url": "https://github.com/yourusername/ollama-jupyter-ai/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Your Name"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ollama-jupyter-ai.git"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:lib && yarn run build:labextension:dev",
    "build:prod": "yarn run clean && yarn run build:lib && yarn run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "yarn run clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf dist",
    "clean:all": "yarn run clean:lib && yarn run clean:labextension",
    "install:extension": "yarn run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0",
    "@jupyterlab/services": "^7.3.5",
    "@jupyterlab/ui-components": "^4.0.0",
    "@lumino/widgets": "^2.0.0",
    "axios": "^1.6.7",
    "lucide-react": "^0.294.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.3.5",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.0",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "dist",
    "sharedPackages": {
      "react": {
        "bundled": false,
        "singleton": true
      },
      "react-dom": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "installConfig": {
    "pnp": false
  }
}
