{
  "name": "jupyterlab-ai-assistant",
  "version": "0.1.0",
  "description": "A JupyterLab extension that integrates Ollama-powered AI assistance directly into notebooks",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "ai",
    "ollama"
  ],
  "homepage": "https://github.com/bhumukul-raj/ollama-ai-assistant-project",
  "bugs": {
    "url": "https://github.com/bhumukul-raj/ollama-ai-assistant-project/issues"
  },
  "license": "MIT",
  "author": {
    "name": "bhumukulraj",
    "email": "bhumukulraj@gmail.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/bhumukul-raj/ollama-ai-assistant-project.git"
  },
  "scripts": {
    "build": "npm run clean && npm run build:lib && npm run build:labextension",
    "build:prod": "npm run clean && npm run build:lib && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:lib": "tsc -b",
    "clean": "npm run clean:lib && npm run clean:labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf jupyterlab_ai_assistant/labextension",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "npm run build",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc -w",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@jupyterlab/application": "^3.6.3",
    "@jupyterlab/apputils": "^3.6.3",
    "@jupyterlab/cells": "^3.6.3",
    "@jupyterlab/coreutils": "^5.6.3",
    "@jupyterlab/mainmenu": "^3.6.3",
    "@jupyterlab/notebook": "^3.6.3",
    "@jupyterlab/services": "^6.6.3",
    "@jupyterlab/settingregistry": "^3.6.3",
    "@jupyterlab/ui-components": "^3.6.3",
    "@lumino/widgets": "^1.37.2",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-markdown": "^8.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "yjs": "^13.5.17"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-syntax-highlighter": "^13.5.2",
    "@types/testing-library__react": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.2",
    "typescript": "~4.9.5"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_ai_assistant/labextension",
    "schemaDir": "schema"
  }
}
